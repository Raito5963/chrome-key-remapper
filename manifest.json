{
  "name": "Qwerty to Dvorak IME",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Qwerty配列をDvorak配列にリマップするIME拡張機能",
  "permissions": [
    "input.ime"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "input_components": [{
    "name": "Qwerty to Dvorak",
    "type": "ime",
    "language": ["en"],
    "layouts": ["qwerty"],
    "id": "qwerty-dvorak-ime"
  }]
}
