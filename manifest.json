{
    "manifest_version": 3,
    "name": "カスタムキーボード拡張",
    "version": "1.1.0",
    "permissions": ["commands"],
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "custom_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "カスタムアクションを実行"
      }
    }
  }
  